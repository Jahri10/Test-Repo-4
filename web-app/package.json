{
  "name": "web-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "analyze": "webpack-bundle-analyzer ./stats.json ./dist",
    "build": "webpack --config webpack.production.js",
    "dev": "webpack serve --config webpack.development.js",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "prepare": "cd .. && husky web-app/.husky",
    "report": "webpack --config webpack.production.js --profile --json > stats.json",
    "serve": "http-server ./dist -a localhost -p 3000 -o --cors",
    "test": "npx jest --coverage"
  },
  "lint-staged": {
    "**/*": "npm run format",
    "src/**/*.{js,jsx,ts,tsx}": "npm run lint:fix"
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.4.6",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/oswald": "^5.0.8",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.142",
    "@mui/material": "^5.14.13",
    "@tanstack/react-query": "^4.32.6",
    "@tanstack/react-query-devtools": "^4.32.6",
    "axios": "^1.6.7",
    "date-fns": "^3.3.1",
    "formik": "^2.4.3",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-drag-drop-files": "^2.3.10",
    "react-router-dom": "^6.22.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.4.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.19",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.4",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.1",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
