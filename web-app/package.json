{
  "name": "web-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "analyze": "webpack-bundle-analyzer ./stats.json ./dist",
    "build": "webpack --config webpack.production.js",
    "dev": "webpack serve --config webpack.development.js",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "prepare": "cd .. && husky web-app/.husky",
    "report": "webpack --config webpack.production.js --profile --json > stats.json",
    "serve": "http-server ./dist -a localhost -p 3000 -o --cors",
    "test": "npx jest --coverage"
  },
  "lint-staged": {
    "**/*": "npm run format",
    "src/**/*.{js,jsx,ts,tsx}": "npm run lint:fix"
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.8.5",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/oswald": "^5.0.19",
    "@fontsource/roboto": "^5.0.12",
    "@mui/icons-material": "^5.15.13",
    "@mui/lab": "^5.0.0-alpha.168",
    "@mui/material": "^5.15.13",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-query-devtools": "^5.28.4",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "formik": "^2.4.5",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-drag-drop-files": "^2.3.10",
    "react-router-dom": "^6.22.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.8.1",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.3",
    "webpack-merge": "^5.10.0"
  }
}
